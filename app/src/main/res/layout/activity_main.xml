<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/id_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:textSize="48sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/id_background"
        android:layout_width="355dp"
        android:layout_height="427dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/background" />

    <ImageView
        android:id="@+id/id_mole1"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginStart="16dp"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toTopOf="@+id/id_mole4"
        app:layout_constraintStart_toStartOf="@+id/id_background"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole2"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toTopOf="@+id/id_mole5"
        app:layout_constraintEnd_toStartOf="@+id/id_mole3"
        app:layout_constraintStart_toEndOf="@+id/id_mole1"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole3"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginEnd="24dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toTopOf="@+id/id_mole6"
        app:layout_constraintEnd_toEndOf="@+id/id_background"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole4"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginStart="16dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintStart_toStartOf="@+id/id_background"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="0.546"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole5"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginEnd="16dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintEnd_toStartOf="@+id/id_mole6"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/id_mole4"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="0.546"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole6"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginEnd="24dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintEnd_toEndOf="@+id/id_background"
        app:layout_constraintTop_toTopOf="@+id/id_background"
        app:layout_constraintVertical_bias="0.546"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole7"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="60dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintStart_toStartOf="@+id/id_background"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole8"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginBottom="60dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintEnd_toStartOf="@+id/id_mole9"
        app:layout_constraintStart_toEndOf="@+id/id_mole7"
        app:srcCompat="@drawable/mole" />

    <ImageView
        android:id="@+id/id_mole9"
        android:layout_width="92dp"
        android:layout_height="92dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="60dp"
        android:visibility="visible"
        android:animateLayoutChanges="true"
        app:layout_constraintBottom_toBottomOf="@+id/id_background"
        app:layout_constraintEnd_toEndOf="@+id/id_background"
        app:srcCompat="@drawable/mole" />

    <TextView
        android:id="@+id/id_score"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_background" />

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="414dp"
        android:layout_height="45dp"
        android:layout_marginBottom="16dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/id_score"></LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>